#################################################################
#    Object Properties
#################################################################

###  http://linked.earth/ontology#calibratedVia
:calibratedVia rdf:type owl:ObjectProperty ;
               rdfs:domain :Variable ;
               rdfs:range :Calibration ;
               rdfs:label "calibrated via" .


###  http://linked.earth/ontology#hasArchiveType
:hasArchiveType rdf:type owl:ObjectProperty ;
                rdfs:domain :Dataset ,
                            :Variable ;
                rdfs:range <http://linked.earth/ontology/archive#ArchiveType> ;
                <http://purl.org/vocab/vann/example> <http://wiki.linked.earth/MD982181.Khider.2014> ;
                <http://www.linkedmodel.org/schema/vaem#rationale> "This property connects a dataset or variable to its archive type. This enables users to search for data from specific archive types (e.g., corals, ice cores)."@en ;
                rdfs:comment "Answers the question: What type of natural archive contains this data?" ;
                rdfs:label "has archive type" .


###  http://linked.earth/ontology#hasAuthor
:hasAuthor rdf:type owl:ObjectProperty ;
           rdfs:domain :Publication ;
           rdfs:range :Person ;
           <http://purl.org/vocab/vann/example> <http://wiki.linked.earth/Mann.etal.1998> ;
           <http://www.linkedmodel.org/schema/vaem#rationale> "Authors are a critical element in the scientific workflow as they are the foundation of scientific credit. The LinkedEarth Ontology seeks to appropriately credit authors and their scientific contributions."@en ;
           rdfs:comment "Links a publication to one of its authors. This property is used to connect all authors to a publication related to the dataset." ;
           rdfs:label "has author" .


###  http://linked.earth/ontology#hasChangeLog
:hasChangeLog rdf:type owl:ObjectProperty ;
              rdfs:domain :Dataset ;
              rdfs:range :ChangeLog ;
              rdfs:label "has change log" .


###  http://linked.earth/ontology#hasChanges
:hasChanges rdf:type owl:ObjectProperty ;
            rdfs:domain :ChangeLog ;
            rdfs:range :Change ;
            rdfs:label "has changes" .


###  http://linked.earth/ontology#hasChronData
:hasChronData rdf:type owl:ObjectProperty ;
              rdfs:domain :Dataset ;
              rdfs:range :ChronData ;
              <http://purl.org/vocab/vann/example> <http://wiki.linked.earth/MD982181.Khider.2014> ;
              <http://www.linkedmodel.org/schema/vaem#rationale> "Subproperty of hasData. The subdivision between chronological and paleoenvironmental information reflects the fact that Datasets are fundamentally characterized by two distinct components"@en ;
              rdfs:comment "Links a dataset to its chronology data. This property connects the dataset to its age model and age-depth measurements." ;
              rdfs:label "has chron data" .


###  http://linked.earth/ontology#hasCode
:hasCode rdf:type owl:ObjectProperty ;
         rdfs:domain :Model ;
         rdfs:label "has code" .


###  http://linked.earth/ontology#hasContributor
:hasContributor rdf:type owl:ObjectProperty ;
                rdfs:domain :Dataset ;
                rdfs:range :Person ;
                rdfs:comment "Links a dataset to a person who contributed to its creation, curation, or publication. Contributors include anyone who helped with the dataset beyond the principal investigators." ;
                rdfs:label "has contributor" .


###  http://linked.earth/ontology#hasCreator
:hasCreator rdf:type owl:ObjectProperty ;
            rdfs:domain :Dataset ;
            rdfs:range :Person ;
            rdfs:label "has creator" .


###  http://linked.earth/ontology#hasCurator
:hasCurator rdf:type owl:ObjectProperty ;
            rdfs:domain :ChangeLog ;
            rdfs:label "has curator" .


###  http://linked.earth/ontology#hasDistributionTable
:hasDistributionTable rdf:type owl:ObjectProperty ;
                      rdfs:domain :Model ;
                      rdfs:range :DataTable ;
                      rdfs:label "has distribution table" .


###  http://linked.earth/ontology#hasEnsembleTable
:hasEnsembleTable rdf:type owl:ObjectProperty ;
                  rdfs:domain :Model ;
                  rdfs:range :DataTable ;
                  rdfs:label "has ensemble table" .


###  http://linked.earth/ontology#hasFirstAuthor
:hasFirstAuthor rdf:type owl:ObjectProperty ;
                rdfs:domain :Publication ;
                rdfs:range :Person ;
                rdfs:label "has first author" .


###  http://linked.earth/ontology#hasFunding
:hasFunding rdf:type owl:ObjectProperty ;
            rdfs:domain :Dataset ;
            rdfs:range :Funding ;
            rdfs:label "has funding" .


###  http://linked.earth/ontology#hasFundingAgency
:hasFundingAgency rdf:type owl:ObjectProperty ;
                  rdfs:domain :Funding ;
                  rdfs:label "has funding agency" .


###  http://linked.earth/ontology#hasIGSN
:hasIGSN rdf:type owl:ObjectProperty ;
         rdfs:domain :PhysicalSample ;
         rdfs:label "has IGSN" .


###  http://linked.earth/ontology#hasInstrument
:hasInstrument rdf:type owl:ObjectProperty ;
               rdfs:domain :Variable ;
               rdfs:label "has instrument" .


###  http://linked.earth/ontology#hasInterpretation
:hasInterpretation rdf:type owl:ObjectProperty ;
                   rdfs:domain :Variable ;
                   rdfs:range :Interpretation ;
                   <http://purl.org/vocab/vann/example> <http://wiki.linked.earth/MD982181.Khider.2014.paleo1measurement2.sst> ;
                   <http://www.linkedmodel.org/schema/vaem#rationale> "This property connects variables to their interpretation. Variables may have multiple interpretations, especially when different researchers propose different interpretations of the same data."@en ;
                   rdfs:comment "Answers the question: What is the climatic or environmental interpretation of this variable?" ;
                   rdfs:label "has interpretation" .


###  http://linked.earth/ontology#hasInvestigator
:hasInvestigator rdf:type owl:ObjectProperty ;
                 rdfs:domain :Dataset ;
                 rdfs:range :Person ;
                 rdfs:comment "Links a dataset to a principal investigator responsible for the research project that generated the dataset." ;
                 rdfs:label "has investigator" .


###  http://linked.earth/ontology#hasLocation
:hasLocation rdf:type owl:ObjectProperty ;
             rdfs:domain :Dataset ;
             rdfs:range :Location ;
             <http://purl.org/vocab/vann/example> <http://wiki.linked.earth/MD982181.Khider.2014> ;
             <http://www.linkedmodel.org/schema/vaem#rationale> "Paleoclimate archives are tied to particular locations. The spatial aspects of paleoclimate data are critical for regional to global studies."@en ;
             rdfs:comment "Links a dataset to its geographic location. This property connects the dataset to information about where the samples were collected." ;
             rdfs:label "has location" .


###  http://linked.earth/ontology#hasMeasurementTable
:hasMeasurementTable rdf:type owl:ObjectProperty ;
                     rdfs:domain :ChronData ,
                                 :PaleoData ;
                     rdfs:range :DataTable ;
                     rdfs:label "has measurement table" .


###  http://linked.earth/ontology#hasPaleoData
:hasPaleoData rdf:type owl:ObjectProperty ;
              rdfs:domain :Dataset ;
              rdfs:range :PaleoData ;
              <http://purl.org/vocab/vann/example> <http://wiki.linked.earth/MD982181.Khider.2014> ;
              <http://www.linkedmodel.org/schema/vaem#rationale> "Subproperty of hasData. The subdivision between chronological and paleoenvironmental information reflects the fact that Datasets are fundamentally characterized by two distinct components"@en ;
              rdfs:comment "Links a dataset to its paleoclimate data. This property connects the dataset to its measurements and derived variables related to past climate." ;
              rdfs:label "has paleo data" .


###  http://linked.earth/ontology#hasPhysicalSample
:hasPhysicalSample rdf:type owl:ObjectProperty ;
                   rdfs:domain :Variable ;
                   rdfs:range :PhysicalSample ;
                   rdfs:label "has physical sample" .


###  http://linked.earth/ontology#hasProxy
:hasProxy rdf:type owl:ObjectProperty ;
          rdfs:domain :Variable ;
          rdfs:range <http://linked.earth/ontology/paleo_proxy#PaleoProxy> ;
          <http://purl.org/vocab/vann/example> <http://wiki.linked.earth/MD982181.Khider.2014.paleo1measurement1.d18O> ;
          <http://www.linkedmodel.org/schema/vaem#rationale> "This property connects a variable to its proxy type. This enables users to search for all data associated with a specific proxy type (e.g., all Î´18O measurements)."@en ;
          rdfs:comment "Answers the question: What is the proxy type of this variable?" ;
          rdfs:label "has proxy" .


###  http://linked.earth/ontology#hasProxyGeneral
:hasProxyGeneral rdf:type owl:ObjectProperty ;
                 rdfs:domain :Variable ;
                 rdfs:range <http://linked.earth/ontology/paleo_proxy#PaleoProxyGeneral> ;
                 rdfs:label "has proxy general" .


###  http://linked.earth/ontology#hasPublication
:hasPublication rdf:type owl:ObjectProperty ;
                rdfs:domain :Dataset ;
                rdfs:range :Publication ;
                <http://purl.org/vocab/vann/example> <http://wiki.linked.earth/MD982181.Khider.2014> ;
                <http://www.linkedmodel.org/schema/vaem#rationale> "A publication is a critical element in the scientific workflow as it documents the scientific information in the context of the literature. The LinkedEarth Ontology connects Datasets to Publications and Authors."@en ;
                rdfs:comment "Links a dataset to a scientific publication that documents it. This can include original publications, review papers, or other works that cite or analyze the dataset." ;
                rdfs:label "has publication" .


###  http://linked.earth/ontology#hasResolution
:hasResolution rdf:type owl:ObjectProperty ;
