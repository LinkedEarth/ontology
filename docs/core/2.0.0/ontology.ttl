@prefix : <http://linked.earth/ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://linked.earth/ontology#> .

<http://linked.earth/ontology#> rdf:type owl:Ontology ;
                                 owl:versionIRI <http://linked.earth/ontology/core/2.0.0> ;
                                 <http://purl.org/dc/terms/description> "The LiPD Ontology aims to provide a common vocabulary for annotating paleoclimatology data"@en ;
                                 <http://purl.org/vocab/vann/preferredNamespacePrefix> "le" ;
                                 <http://purl.org/vocab/vann/preferredNamespaceUri> "http://linked.earth/ontology#" ;
                                 rdfs:comment "The LiPD Ontology aims to provide a common vocabulary for annotating paleoclimatology data"@en ;
                                 owl:versionInfo "2.0.0"@en .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/terms/description
<http://purl.org/dc/terms/description> rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/example
<http://purl.org/vocab/vann/example> rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespacePrefix
<http://purl.org/vocab/vann/preferredNamespacePrefix> rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespaceUri
<http://purl.org/vocab/vann/preferredNamespaceUri> rdf:type owl:AnnotationProperty .


###  http://www.linkedmodel.org/schema/vaem#rationale
<http://www.linkedmodel.org/schema/vaem#rationale> rdf:type owl:AnnotationProperty .


#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2000/01/rdf-schema#Literal
rdfs:Literal owl:equivalentClass xsd:string .


#################################################################
#    Object Properties
#################################################################

###  http://linked.earth/ontology#generatedByModel
:generatedByModel rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf <http://www.w3.org/ns/prov#wasDerivedFrom> ;
                  rdfs:domain :DataTable ;
                  rdfs:range :Model ;
                  <http://www.linkedmodel.org/schema/vaem#rationale> "Needed to link the model to the data tables it generates"@en ;
                  rdfs:comment "Answers the question: which model was used to generated this data table?"@en .


###  http://linked.earth/ontology#hasArchiveType
:hasArchiveType rdf:type owl:ObjectProperty ;
                rdfs:domain :Dataset .


###  http://linked.earth/ontology#hasAuthor
:hasAuthor rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf <http://purl.org/dc/terms/creator> ,
                              <http://schema.org/author> ;
           rdfs:domain [ rdf:type owl:Class ;
                         owl:unionOf ( :Compilation
                                       :Dataset
                                       :Publication
                                     )
                       ] ;
           rdfs:range :Person ;
           <http://purl.org/vocab/vann/example> <http://wiki.linked.earth/Publication.10.1002/2013PA002534> ;
           <http://www.linkedmodel.org/schema/vaem#rationale> "Give credit to the original author of the resource"@en ;
           rdfs:comment "Answers the question: Who wrote (i.e., created) the resource, such as a Publication?"@en .


###  http://linked.earth/ontology#hasCalibration
:hasCalibration rdf:type owl:ObjectProperty ;
                rdfs:range :Calibration ;
                <http://purl.org/vocab/vann/example> <http://wiki.linked.earth/MD982181.Khider.2014.paleo1measurement2.sst> ;
                <http://www.linkedmodel.org/schema/vaem#rationale> "Provides calibration information, especially useful for legacy datasets where the raw measurements may not be available and for comparison of datasets."@en ;
                rdfs:comment "Answers the question: What calibration process was used to relate the InferredVariable to the MeasuredVariable?"@en .


###  http://linked.earth/ontology#hasChronData
:hasChronData rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf :hasData ;
              rdfs:domain :Dataset ;
              rdfs:range :ChronData ;
              <http://purl.org/vocab/vann/example> <http://wiki.linked.earth/MD982181.Khider.2014> ;
              <http://www.linkedmodel.org/schema/vaem#rationale> "Subproperty of hasData. The subdivision between chronological and paleoenvironmental information reflects the fact that Datasets are fundamentally characterized by two distinct components"@en ;
              rdfs:comment "Answers the question: Which ChronData are included in the Dataset?"@en .


###  http://linked.earth/ontology#hasCode
:hasCode rdf:type owl:ObjectProperty ,
                  owl:FunctionalProperty ;
         rdfs:domain :Model ;
         rdfs:range :SoftwareCode ;
         <http://www.linkedmodel.org/schema/vaem#rationale> "Links a Model to its SoftwareCode."@en ;
         rdfs:comment "Answers the question: What SoftwareCode was used to generate the Model output?"@en .


###  http://linked.earth/ontology#hasContributor
:hasContributor rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf <http://purl.org/dc/terms/contributor> ,
                                   <http://schema.org/contributor> ;
                rdfs:domain :Dataset ;
                rdfs:range :Person ;
                <http://purl.org/vocab/vann/example> <http://wiki.linked.earth/MD982181.Khider.2014> ;
                <http://www.linkedmodel.org/schema/vaem#rationale> "Allows distinction between contributor to a resource and the author"@en ;
                rdfs:comment "Answers the question: Who contributed the resource? A contributor may be the same as the authors of the resource or different. For instance, in the case of a legacy dataset, a Linked Earth member may contribute a previously-published dataset that he/she may not have authored"@en .


###  http://linked.earth/ontology#hasCreator
:hasCreator rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf <http://purl.org/dc/terms/creator> ;
            rdfs:domain [ rdf:type owl:Class ;
                          owl:unionOf ( :Dataset
                                        :SoftwareCode
                                      )
                        ] ;
            rdfs:range :Person .


###  http://linked.earth/ontology#hasData
:hasData rdf:type owl:ObjectProperty ;
         rdfs:domain :Dataset ;
         rdfs:range :Data ;
         <http://www.linkedmodel.org/schema/vaem#rationale> "Links Dataset to Data"@en ;
         rdfs:comment "Answers the question: Which Data are included in the Dataset?"@en .


###  http://linked.earth/ontology#hasDataContributor
:hasDataContributor rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf <http://purl.org/dc/terms/creator> ;
                    rdfs:domain :Dataset ;
                    rdfs:range :Person .


###  http://linked.earth/ontology#hasFirstAuthor
:hasFirstAuthor rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf <http://purl.org/dc/terms/creator> ,
                                   <http://schema.org/author> ;
                rdfs:domain [ rdf:type owl:Class ;
                              owl:unionOf ( :Compilation
                                            :Dataset
                                            :Publication
                                          )
                            ] ;
                rdfs:range :Person .


###  http://linked.earth/ontology#hasFunder
:hasFunder rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf <http://schema.org/funder> ;
           rdfs:domain [ rdf:type owl:Class ;
                         owl:unionOf ( :Compilation
                                       :Dataset
                                     )
                       ] ;
           rdfs:range :Funding ;
           <http://purl.org/vocab/vann/example> <http://wiki.linked.earth/MD982181.Khider.2014> ;
           <http://www.linkedmodel.org/schema/vaem#rationale> "Links a dataset to its source of funding"@en ;
           rdfs:comment "Answers the question: What was the Funding source for the Dataset?"@en .


###  http://linked.earth/ontology#hasInstrument
:hasInstrument rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf <http://purl.oclc.org/NET/ssnx/ssn#observedBy> ;
               rdfs:range :Instrument ;
               <http://purl.org/vocab/vann/example> <http://wiki.linked.earth/MD982181.Khider.2014.paleo1measurement2.mgcarub> ;
               <http://www.linkedmodel.org/schema/vaem#rationale> "Links the MeasuredVariable to the Instrument responsible for its creation"@en ;
               rdfs:comment "Answers the question: Which Instrument was used to produce the MeasuredVariable?"@en .


###  http://linked.earth/ontology#hasIntegrationTime
:hasIntegrationTime rdf:type owl:ObjectProperty ;
                    rdfs:range :IntegrationTime ;
                    <http://www.linkedmodel.org/schema/vaem#rationale> "Property added to link the proxySystem to the IntegrationTime class"@en ;
                    rdfs:comment "Answers the question: What is the integration time needed for a ProxySensor to imprint environmental information on the ProxyArchive? Both are included as part of a ProxySystem"@en .


###  http://linked.earth/ontology#hasInterpretation
:hasInterpretation rdf:type owl:ObjectProperty ;
                   rdfs:domain :Variable ;
                   rdfs:range :Interpretation ;
                   <http://purl.org/vocab/vann/example> <http://wiki.linked.earth/MD982181.Khider.2014.paleo1measurement2.sst> ;
                   <http://www.linkedmodel.org/schema/vaem#rationale> "Links the Variable to its Interpretation"@en ;
                   rdfs:comment "Answers the question: What is the Variable's Interpretation?"@en .


###  http://linked.earth/ontology#hasInvestigator
:hasInvestigator rdf:type owl:ObjectProperty ;
                 rdfs:domain :Funding ;
                 rdfs:range :Person ;
                 <http://purl.org/vocab/vann/example> <http://wiki.linked.earth/MD982181.Khider.2014.Funding1> ;
                 <http://www.linkedmodel.org/schema/vaem#rationale> "Links the source of Funding to the PI (a Person)"@en ;
                 rdfs:comment "Answers the question: Who was the lead on the source of Funding for the Dataset?"@en .


###  http://linked.earth/ontology#hasLocation
:hasLocation rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf <http://schema.org/contentLocation> ;
             rdfs:domain :Dataset ;
             rdfs:range :Location ;
             <http://purl.org/vocab/vann/example> <http://wiki.linked.earth/MD982181.Khider.2014> ;
             <http://www.linkedmodel.org/schema/vaem#rationale> "Describe the geographical coordinates for the provenance of the ProxyArchive where the dataset was collected from"@en ;
             rdfs:comment "Answers the question: Where (in the world) was the data in the Dataset collected? Refers to the location of the ProxyArchive"@en .


###  http://linked.earth/ontology#hasMeasurementTable
:hasMeasurementTable rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf :hasTable ;
                     rdfs:domain :Data ;
                     rdfs:range :MeasurementTable ;
                     <http://purl.org/vocab/vann/example> <http://wiki.linked.earth/MD982181.Khider.2014.PaleoData1> ;
                     <http://www.linkedmodel.org/schema/vaem#rationale> "Subproperty of foundinTable to link the Data to the Variables"@en ;
                     rdfs:comment "Answers the question: In which MeasurementTable are the Variables found?"@en .


###  http://linked.earth/ontology#hasPaleoData
:hasPaleoData rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf :hasData ;
              rdfs:domain :Dataset ;
              rdfs:range :PaleoData ;
              <http://purl.org/vocab/vann/example> <http://wiki.linked.earth/MD982181.Khider.2014> ;
              <http://www.linkedmodel.org/schema/vaem#rationale> "Subproperty of hasData. The subdivision between chronological and paleoenvironmental information reflects the fact that Datasets are fundamentally characterized by two distinct components"@en ;
              rdfs:comment "Answers the question: Which PaleoData are included in the Dataset?"@en .


###  http://linked.earth/ontology#hasProxy
:hasProxy rdf:type owl:ObjectProperty ;
          rdfs:domain :Variable .


###  http://linked.earth/ontology#hasProxyGeneral
:hasProxyGeneral rdf:type owl:ObjectProperty ;
                 rdfs:domain :Variable .


###  http://linked.earth/ontology#hasPublication
:hasPublication rdf:type owl:ObjectProperty ;
                rdfs:domain :Dataset ;
                rdfs:range :Publication ;
                <http://purl.org/vocab/vann/example> <http://wiki.linked.earth/MD982181.Khider.2014> ;
                <http://www.linkedmodel.org/schema/vaem#rationale> "Links the resource to its Publication"@en ;
                rdfs:comment "Answers the question: In which Publication was the Dataset or Compilation issued?"@en .


###  http://linked.earth/ontology#hasReferences
:hasReferences rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf <http://purl.org/dc/terms/references> ;
               rdfs:domain :Calibration ;
               rdfs:range :Publication ;
               <http://purl.org/vocab/vann/example> <http://wiki.linked.earth/MD982181.Khider.2014.paleo1measurement1.mgcarub.Calibration> ;
               <http://www.linkedmodel.org/schema/vaem#rationale> "Relationship needed to link to a Publication desribing the calibration being applied to the dataset"@en ;
               rdfs:comment "Answers the question: In what Reference(s) is the Calibration described/justified?"@en .


###  http://linked.earth/ontology#hasResolution
:hasResolution rdf:type owl:ObjectProperty ;
               rdfs:domain :Variable ;
               rdfs:range :Resolution ;
               <http://purl.org/vocab/vann/example> "http://wiki.linked.earth/MD982181.Khider.2014.paleo1measurement1.mgcarub.ICPAES"@en ;
               <http://www.linkedmodel.org/schema/vaem#rationale> "Needed to link a Variable to Resolution"@en ;
               rdfs:comment "Answers the question: What is the Resolution of the Variable?" .


###  http://linked.earth/ontology#hasTable
:hasTable rdf:type owl:ObjectProperty ;
          rdfs:domain [ rdf:type owl:Class ;
                        owl:unionOf ( :Data
                                      :Variable
                                    )
                      ] ;
          rdfs:range :DataTable ;
          <http://www.linkedmodel.org/schema/vaem#rationale> "Allows to link the data to the csv table and the models to its output"@en ;
          rdfs:comment "Answers the question: In which DataTable can the information/data be found?"@en .


###  http://linked.earth/ontology#hasUncertainty
:hasUncertainty rdf:type owl:ObjectProperty ;
                rdfs:domain [ rdf:type owl:Class ;
                              owl:unionOf ( :Calibration
                                            :Instrument
                                            :Model
                                            :Variable
                                          )
                            ] ;
                rdfs:range :Uncertainty ;
                <http://purl.org/vocab/vann/example> <http://wiki.linked.earth/MD982181.Khider.2014.paleo1measurement1.mgcarub> ;
                <http://www.linkedmodel.org/schema/vaem#rationale> "Links to the Uncertainty class"@en ;
                rdfs:comment "Answers the question: What is the Uncertainty associated with the Variable, Model, or Instrument?"@en .


###  http://linked.earth/ontology#hasVariable
:hasVariable rdf:type owl:ObjectProperty ;
             rdfs:domain :DataTable ;
             rdfs:range :Variable ;
             <http://purl.org/vocab/vann/example> <http://wiki.linked.earth/MD982181.Khider.2014.paleo1measurement2> ;
             <http://www.linkedmodel.org/schema/vaem#rationale> "Links DataTable to Variable"@en ;
             rdfs:comment "Answers the question: Which Variables are included in the DataTable?"@en .


###  http://linked.earth/ontology#inCompilation
:inCompilation rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf <http://purl.org/dc/terms/isPartOf> ,
                                  <http://schema.org/includedInDataCatalog> ;
               rdfs:domain :Dataset ;
               rdfs:range :Compilation ;
               <http://www.linkedmodel.org/schema/vaem#rationale> "Links the dataset to the compilation"@en ;
               rdfs:comment "Answer the question: Which compilation does a dataset belong to?"@en .


###  http://linked.earth/ontology#modeledBy
:modeledBy rdf:type owl:ObjectProperty ;
           rdfs:range :Model ;
           <http://www.linkedmodel.org/schema/vaem#rationale> "Links Data, Compilation or ProxySystem to Model"@en ;
           rdfs:comment "Answers the question: What is the Model for the Data, Compilation or ProxySystem under consideration?"@en .


###  http://linked.earth/ontology#reportedIn
:reportedIn rdf:type owl:ObjectProperty ;
            rdfs:domain :Uncertainty ;
            rdfs:range [ rdf:type owl:Class ;
                         owl:unionOf ( :DataTable
                                       :Variable
                                     )
                       ] ;
            <http://purl.org/vocab/vann/example> <http://wiki.linked.earth/MD982181.Khider.2014.paleo1measurement2.sst.Uncertainty> ;
            <http://www.linkedmodel.org/schema/vaem#rationale> "Links the uncertainty to the model output"@en ;
            rdfs:comment "Answers the question: In which EnsembleTable, DistributionTable, and/or SummaryTable is the Uncertainty reported?"@en .


###  http://purl.oclc.org/NET/ssnx/ssn#observedBy
<http://purl.oclc.org/NET/ssnx/ssn#observedBy> rdf:type owl:ObjectProperty ;
                                               rdfs:isDefinedBy <http://purl.oclc.org/NET/ssnx/ssn#> .


###  http://purl.org/dc/terms/contributor
<http://purl.org/dc/terms/contributor> rdf:type owl:ObjectProperty ;
                                       rdfs:isDefinedBy <http://purl.org/dc/terms/> .


###  http://purl.org/dc/terms/creator
<http://purl.org/dc/terms/creator> rdf:type owl:ObjectProperty ;
                                   rdfs:isDefinedBy <http://purl.org/dc/terms/> .


###  http://purl.org/dc/terms/isPartOf
<http://purl.org/dc/terms/isPartOf> rdf:type owl:ObjectProperty ;
                                    rdfs:isDefinedBy <http://purl.org/dc/terms/> .


###  http://purl.org/dc/terms/references
<http://purl.org/dc/terms/references> rdf:type owl:ObjectProperty ;
                                      rdfs:isDefinedBy <http://purl.org/dc/terms/> .


###  http://schema.org/author
<http://schema.org/author> rdf:type owl:ObjectProperty ;
                           rdfs:isDefinedBy <http://schema.org/author> .


###  http://schema.org/contentLocation
<http://schema.org/contentLocation> rdf:type owl:ObjectProperty ;
                                    rdfs:isDefinedBy <http://schema.org/> .


###  http://schema.org/contributor
<http://schema.org/contributor> rdf:type owl:ObjectProperty ;
                                rdfs:isDefinedBy <http://schema.org/> .


###  http://schema.org/funder
<http://schema.org/funder> rdf:type owl:ObjectProperty ;
                           rdfs:isDefinedBy <http://schema.org/> .


###  http://schema.org/includedInDataCatalog
<http://schema.org/includedInDataCatalog> rdf:type owl:ObjectProperty ;
                                          rdfs:isDefinedBy <http://schema.org/> .


###  http://www.opengis.net/ont/geosparql#hasGeometry
<http://www.opengis.net/ont/geosparql#hasGeometry> rdf:type owl:ObjectProperty ;
                                                   rdfs:domain <http://www.opengis.net/ont/geosparql#Feature> ;
                                                   rdfs:range <http://www.opengis.net/ont/geosparql#Geometry> ;
                                                   rdfs:isDefinedBy <http://www.opengis.net/spec/geosparql/1.0> .


###  http://www.w3.org/ns/prov#wasDerivedFrom
<http://www.w3.org/ns/prov#wasDerivedFrom> rdf:type owl:ObjectProperty ;
                                           rdfs:isDefinedBy <http://www.w3.org/ns/prov#> .


#################################################################
#    Data properties
#################################################################

###  http://linked.earth/ontology#description
:description rdf:type owl:DatatypeProperty ;
             rdfs:domain owl:Thing ;
             rdfs:range xsd:string ;
             <http://purl.org/vocab/vann/example> <http://wiki.linked.earth/MD982181.Khider.2014.paleo1measurement1.mgcarub> ;
             <http://www.linkedmodel.org/schema/vaem#rationale> "This property may be used to provide further details on instrument descriptions, uncertainty description,etc."@en ;
             rdfs:comment "Answers the question: What additional details would you give about the resource?"@en .


###  http://linked.earth/ontology#detail
:detail rdf:type owl:DatatypeProperty ;
        rdfs:domain :Interpretation ;
        rdfs:range xsd:string ;
        <http://purl.org/vocab/vann/example> <http://wiki.linked.earth/MD982181.Khider.2014.paleo1measurement1.mgcarub.Interpretation> ;
        rdfs:comment "Answers the  \"of what\" question for an Interpretation. For intance, sea surface in sea surface temperature."@en .


###  http://linked.earth/ontology#email
:email rdf:type owl:DatatypeProperty ;
       rdfs:domain :Person ;
       rdfs:range xsd:string ;
       <http://purl.org/vocab/vann/example> <http://wiki.linked.earth/D._Khider> ;
       <http://www.linkedmodel.org/schema/vaem#rationale> "Allow to store contact information"@en ;
       rdfs:comment "Answers the question: What is the email address of the Person?"@en .


###  http://linked.earth/ontology#hasAbstract
:hasAbstract rdf:type owl:DatatypeProperty ;
             rdfs:domain :Publication ;
             rdfs:range xsd:string .


###  http://linked.earth/ontology#hasBook
:hasBook rdf:type owl:DatatypeProperty ;
         rdfs:domain :Publication ;
         rdfs:range xsd:string ;
         <http://www.linkedmodel.org/schema/vaem#rationale> "Separate between journal and book publication"@en ;
         rdfs:comment "Answers the question: In which book was the resource published?"@en .


###  http://linked.earth/ontology#hasCitation
:hasCitation rdf:type owl:DatatypeProperty ;
             rdfs:subPropertyOf <http://purl.org/dc/terms/bibliographicCitation> ,
                                <http://schema.org/citation> ;
             rdfs:domain [ rdf:type owl:Class ;
                           owl:unionOf ( :Compilation
                                         :Dataset
                                         :Publication
                                       )
                         ] ;
             rdfs:range xsd:string ;
             <http://www.linkedmodel.org/schema/vaem#rationale> "Data property needed to specify how to cite datasets, publications, or compilations, in order to provide appropriate credit to all contributors"@en ;
             rdfs:comment "Answers the question: How should someone cite the dataset, publication, or compilation ?"@en .


###  http://linked.earth/ontology#hasCiteKey
:hasCiteKey rdf:type owl:DatatypeProperty ;
            rdfs:subPropertyOf <http://purl.org/dc/terms/bibliographicCitation> ,
                               <http://schema.org/citation> ;
            rdfs:domain :Publication ;
            rdfs:range xsd:string .


###  http://linked.earth/ontology#hasCollectionName
:hasCollectionName rdf:type owl:DatatypeProperty ;
                   rdfs:domain :Dataset ;
                   rdfs:range xsd:string .


###  http://linked.earth/ontology#hasCollectionYear
:hasCollectionYear rdf:type owl:DatatypeProperty ;
                   rdfs:domain :Dataset ;
                   rdfs:range xsd:integer .


###  http://linked.earth/ontology#hasColumnNumber
:hasColumnNumber rdf:type owl:DatatypeProperty ;
                 rdfs:domain :Variable ;
                 rdfs:range xsd:integer ;
                 <http://purl.org/vocab/vann/example> <http://wiki.linked.earth/MD982181.Khider.2014.paleo1measurement1.mgcarub> ;
                 <http://www.linkedmodel.org/schema/vaem#rationale> "Links the Variable metadata to the values stored in the csv file"@en ;
                 rdfs:comment "Answers the question: In which column of the csv file can the Variable be found?"@en .


###  http://linked.earth/ontology#hasCompilationNest
:hasCompilationNest rdf:type owl:DatatypeProperty ;
                    rdfs:domain :Dataset ;
                    rdfs:range xsd:string .


###  http://linked.earth/ontology#hasDOI
:hasDOI rdf:type owl:DatatypeProperty ;
        rdfs:subPropertyOf <http://purl.org/dc/terms/identifier> ;
        rdf:type owl:FunctionalProperty ;
        rdfs:domain [ rdf:type owl:Class ;
                      owl:unionOf ( :Calibration
                                    :Dataset
                                    :Publication
                                    :SoftwareCode
                                  )
                    ] ;
        rdfs:range xsd:string ;
        <http://purl.org/vocab/vann/example> <http://wiki.linked.earth/Publication.10.1002/2013PA002534> ;
        <http://www.linkedmodel.org/schema/vaem#rationale> "Allows to link to a resource using a digital object identifier"@en ;
        rdfs:comment "Answers the question: What is the digital object identifier associated with the resource?"@en .


###  http://linked.earth/ontology#hasDataUrl
:hasDataUrl rdf:type owl:DatatypeProperty ;
            rdfs:domain :Publication ;
            rdfs:range xsd:string .


###  http://linked.earth/ontology#hasDatasetId
:hasDatasetId rdf:type owl:DatatypeProperty ;
              rdfs:domain :Dataset ;
              rdfs:range xsd:string ;
              <http://www.linkedmodel.org/schema/vaem#rationale> "Allows assigning external ids to datasets"@en ;
              rdfs:comment "Associates a dataset with an external identifier"@en .


###  http://linked.earth/ontology#hasDatasetLicense
:hasDatasetLicense rdf:type owl:DatatypeProperty ;
                   rdfs:subPropertyOf <http://purl.org/dc/terms/license> ,
                                      <http://schema.org/license> ;
                   rdfs:domain :Dataset ;
                   rdfs:range xsd:anyURI ;
                   <http://www.linkedmodel.org/schema/vaem#rationale> "Property needed to let other scientists know about the usage and attribution terms of the uploaded dataset"@en ;
                   rdfs:comment "License associated to a dataset"@en .


###  http://linked.earth/ontology#hasDatasetRange
:hasDatasetRange rdf:type owl:DatatypeProperty ;
                 rdfs:domain :Calibration ;
                 rdfs:range xsd:string .


###  http://linked.earth/ontology#hasDate
:hasDate rdf:type owl:DatatypeProperty ;
